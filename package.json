{
  "name": "pupsteps",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:clean": "cd android && gradlew clean",
    "android:dev": "npx react-native run-android --variant=devDebug --appIdSuffix dev",
    "android:alpha": "npm ci && cd android && gradlew assembleAlphaDebug && cd .. && start android\\app\\build\\outputs\\apk\\alpha\\debug",
    "android:beta": "npm ci && cd android && gradlew bundleBetaRelease && cd .. && start android\\app\\build\\outputs\\bundle\\betaRelease",
    "android:prod": "npm ci && cd android && gradlew bundleProdRelease && cd .. && start android\\app\\build\\outputs\\bundle\\prodRelease"
  },
  "dependencies": {
    "@react-native-firebase/app": "^18.1.0",
    "@react-native-firebase/auth": "^18.1.0",
    "@react-native-google-signin/google-signin": "^10.0.1",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-config": "^1.5.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "^3.22.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.1.1",
    "redux-saga": "^1.2.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.0",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
